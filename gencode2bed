#!/usr/bin/python

import sys

annotation=sys.argv[1]

def gtfinfo_to_dict(info):
    pairs = info.strip().split("; ")
    results = {}
    for pair in pairs:
        key, value = pair.strip().split(" ")
        value = value.replace("\"", "")
        results[key] = value
    return results

for line in sys.stdin:
    if line.startswith("#"):
        continue
    fields = line.strip().split("\t")
    chrom = fields[0]
    start = int(fields[3]) - 1
    end = fields[4] # bed is 0-based half-open
    info = fields[8]
    annotations = gtfinfo_to_dict(info)
    value = annotations[annotation]
    print "\t".join(map(str, [chrom, start, end, value]))
